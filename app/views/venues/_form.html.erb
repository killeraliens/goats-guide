<h2>Add New Venue</h2>

<%= simple_form_for @venue do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :country, as: :string %>
    <%= f.input :info %>
  </div>


  <div class="form-actions">
    <%= f.button :submit, "Add Venue" %>
  </div>
<% end %>

