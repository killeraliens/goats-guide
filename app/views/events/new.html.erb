<h2>Create Event</h2>
<%= simple_form_for @event do |f| %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :date %>
    <!-- <#%= f.association :venue %> -->
  </div>
  <div class="form-actions">
    <%= f.button :submit, "Add Event" %>
  </div>
  <%= render "venues/form" %>
<% end %>

